{
  "name": "Bookmark Viewer",
  "description": "북마크 관리 확장 프로그램",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon-bookmark-16.png",
    "32": "icon-bookmark-32.png",
    "48": "icon-bookmark-64.png",
    "128": "icon-bookmark-128.png"
  }
}